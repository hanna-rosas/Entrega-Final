  let cart = [];

  // Funci√≥n para mostrar los c√≥mics en la galer√≠a
  function displayComics(list) {
    const gallery = document.getElementById('comics-gallery');
    gallery.innerHTML = ''; // Limpiar
    if(list.length === 0){
      gallery.innerHTML = '<p style="color:#ff4444; font-weight:bold; text-align:center; width:100%;">No se encontraron c√≥mics.</p>';
      return;
    }
    list.forEach(comic => {
      const card = document.createElement('div');
      card.className = 'comic-card';
      card.innerHTML = `
        <img src="${comic.image}" alt="${comic.title}" />
        <div class="comic-info">
          <h3>${comic.title}</h3>
          <p>${comic.description}</p>
          <p><strong>$${comic.price.toFixed(2)}</strong></p>
          <button onclick="addToCart(${comic.id})">Agregar al carrito</button>
        </div>
      `;
      gallery.appendChild(card);
    });
  }

  // Filtrar c√≥mics por categor√≠a
  function filterCategory(cat) {
    const buttons = document.querySelectorAll('#categories button');
    buttons.forEach(btn => btn.classList.remove('active'));
    buttons.forEach(btn => {
      if(btn.textContent.toLowerCase() === cat || (cat === 'all' && btn.textContent.toLowerCase() === 'todos')) {
        btn.classList.add('active');
      }
    });
    if(cat === 'all') {
      displayComics(comics);
    } else {
      displayComics(comics.filter(c => c.category === cat));
    }
  }

  // Buscar c√≥mics por texto
  function searchComics() {
    const query = document.getElementById('searchInput').value.trim().toLowerCase();
    if(query === ''){
      filterCategory('all');
      return;
    }
    const results = comics.filter(c => c.title.toLowerCase().includes(query) || c.description.toLowerCase().includes(query));
    displayComics(results);
  }

  // Agregar c√≥mic al carrito
  function addToCart(id) {
    const comic = comics.find(c => c.id === id);
    if(!comic) return;
    const cartItem = cart.find(item => item.id === id);
    if(cartItem){
      cartItem.quantity++;
    } else {
      cart.push({...comic, quantity: 1});
    }
    alert(`${comic.title} agregado al carrito`);
    updateCartUI();
  }

  // Actualizar el UI del carrito
  function updateCartUI() {
    const cartList = document.getElementById('cart-list');
    cartList.innerHTML = '';
    if(cart.length === 0){
      cartList.innerHTML = '<li>Tu carrito est√° vac√≠o.</li>';
      return;
    }
    cart.forEach(item => {
      const li = document.createElement('li');
      li.textContent = `${item.title} x${item.quantity} - $${(item.price * item.quantity).toFixed(2)}`;
      cartList.appendChild(li);
    });
  }

  // Mostrar/ocultar carrito
  function toggleCart() {
    const cartDiv = document.getElementById('cart');
    cartDiv.classList.toggle('active');
  }

  // Finalizar compra (solo mensaje)
  function finalizePurchase() {
    if(cart.length === 0){
      alert('El carrito est√° vac√≠o.');
      return;
    }
    const total = cart.reduce((sum, item) => sum + item.price * item.quantity, 0);
    alert(`Gracias por tu compra. Total a pagar: $${total.toFixed(2)}`);
    cart = [];
    updateCartUI();
    toggleCart();
  }

  // Inicializaci√≥n
  displayComics(comics);
</script>

</body>
</html>
